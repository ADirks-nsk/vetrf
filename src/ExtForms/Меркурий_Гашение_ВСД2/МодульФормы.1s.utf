		//тзСоответствия.ВладелецХС = тз.ВладелецХС;
		//тзСоответствия.НаименованиеПродукции = тз.НаименованиеПродукции;
		тзСоответствия.Номенклатура = тз.Номенклатура;
		тзСоответствия.Продукция_Элемент = тз.Продукция_Элемент;		
