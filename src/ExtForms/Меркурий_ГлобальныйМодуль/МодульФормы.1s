					Пауза( );
					Результат = ПолучитьРезультат_ВСД_Исходящий( ДокОб.ТекущийДокумент() );
				КонецЕсли;
			КонецЦикла;
			
			//проведём для красоты
			ДокОб = СоздатьОбъект("Документ");
			ДокОб.НайтиДокумент(док);
