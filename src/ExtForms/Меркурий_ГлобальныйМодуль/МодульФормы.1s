		СохранитьСтатусЗапроса(док);		
		
		Если Результат="ACCEPTED" Тогда 
			//ВСД отправлен успешно, осталось получить ответ				
			Пауза();			
					
			Результат = ПолучитьРезультат_ВСД_Исходящий( ДокОб.ТекущийДокумент() );
