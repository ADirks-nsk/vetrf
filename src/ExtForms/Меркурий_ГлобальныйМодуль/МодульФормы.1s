		Исключение
			//Сообщить("Не удалось получить элемент [packingForm] [uuid]","!");
			packingFormUuid = "";			
		КонецПопытки;
		Попытка
			packingFormName	= batch.selectSingleNode("*[local-name()='packingList']").selectSingleNode("*[local-name()='packingForm']").selectSingleNode("*[local-name()='name']").text;			
