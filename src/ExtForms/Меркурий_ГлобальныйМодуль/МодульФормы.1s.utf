		СпрОбъект.ТермическиеУсловияПеревозки = СписокКонстант.Получить("ТермУсловияПеревозки");//1;
		
		СпрОбъект.Записать();

		Сообщить("Записан ["+СпрОбъект.ТекущийЭлемент()+"] uuid =["+uuid+"]");
	КонецЦикла;
				
	УдалитьXML( Компонента.LogFilename );

	Возврат "";
