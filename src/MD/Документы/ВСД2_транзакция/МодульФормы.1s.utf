				ИмяФайлаМаршрутДоставки = ГМ2.ПолучитьИмяФайла_МаршрутДоставки( ТекущийДокумент() );
				Записать();
			КонецЕсли;

			КаталогОбработки = ГМ.СписокКонстант.Получить("КаталогМодуля");				
			СписокПараметров = СоздатьОбъект("СписокЗначений");
