Процедура ВывестиСекциюВСД(Таб, ВСД)
	
	Если ПустоеЗначение(ВСД.UUID)=1 Тогда 
		Сообщить("Пропущен "+ВСД+" - "+ВСД.Статус+" - UUID ["+ВСД.UUID+"]","!");
		Возврат;
	КонецЕсли;
	
	тВСД = "Ветеринарное свидетельство (оформлен) Код: "+ВСД.UUID;
	тОтправитель = "Фирма-отправитель: "+ ВСД.Отправитель_ХозСубъект.Контрагент.ЮрФизЛицо.ПолноеНаименование()+", ИНН:"+ 
		ВСД.Отправитель_ХозСубъект.Контрагент.ЮрФизЛицо.ИНН+", ТТН: № "+ВСД.ТТНномер+" от "+ВСД.ТТНДата;
	тПолучатель = "Фирма-получатель: "+ВСД.Получатель_ХозСубъект.Контрагент.ЮрФизЛицо.ПолнНаименование+", ИНН: "+
		ВСД.Получатель_ХозСубъект.Контрагент.ЮрФизЛицо.ИНН; 
		
	Попытка
		тПлощадка = "Предприятие-получатель: "+ВСД.Получатель_Площадка.Контрагент.ЮрФизЛицо.ПолнНаименование+" - "+ВСД.Получатель_ХозСубъект.Контрагент.ЮрФизЛицо.ФактАдрес;
	Исключение
		тПлощадка = "Предприятие-получатель: "+ВСД.Получатель_Площадка+" - "+ВСД.Получатель_ХозСубъект.Контрагент.ЮрФизЛицо.ФактАдрес;
	КонецПопытки;
	тПродукция = "Продукция: "+ СокрЛП(ВСД.НаименованиеПродукции)+", "+ВСД.Количество+" кг";   
	тВыработано = "Выработана: " + СокрЛП(ВСД.ДатаИзготовления1) + "-" + СокрЛП(ВСД.ДатаИзготовления2) + ", ";
	тКод = "Код: "+ВСД.UUID;  
	тСгенерировано = "Сгенерировано системой '1С' 7.7 "+ТекущаяДата()+" "+ТекущееВремя()+" "+глПользователь;
   			 			
   			
	Сервис = СоздатьОбъект("Сервис");
	шк = "http://mercury.vetrf.ru/pub/operatorui?_language=ru&_action=showVetDocumentFormByUuid&uuid="+ СокрЛП(ВСД.UUID);
   	
	Таб.ВывестиСекцию("Штрихкод");  
	
КонецПроцедуры

