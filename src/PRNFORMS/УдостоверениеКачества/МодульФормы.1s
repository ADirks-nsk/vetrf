Процедура ПечатьСертификат(Докум, Устройство=0, КолвоКопий=0, СразуНаПринтер=0) 
	//******************************************************************************   
	
	ТЗ = СоздатьОбъект("ТаблицаЗначений");
	ТЗ.НоваяКолонка("Сертификат");
	
   Таб = СоздатьОбъект("Таблица");
   Таб.ИсходнаяТаблица("Таблица");
   Таб.ВывестиСекцию("Сек_1");
   Докум.ВыбратьСтроки(); 
   Пока Докум.ПолучитьСтроку()=1 Цикл
   	 	Сертификат = Докум.Номенклатура.СпрСертификат;
   	 	Если ПустоеЗначение(Сертификат)=1 Тогда 
   	  		Сообщить("Не указан сертификат у товара "+Докум.Номенклатура);
   	 	Иначе
   	  		ТЗ.НоваяСтрока();
			ТЗ.Сертификат = Сертификат;
   	 	КонецЕсли;   
   	  
   	  	ДатаВыработки = Докум.Номенклатура.ДатаИзготовления;

   	  
    	Таб.ВывестиСекцию("Сек_2");
   	КонецЦикла; 
     
   	Таб.ВывестиСекцию("Сек_3"); 
   	
   	ВСД="";
	Доки =  СоздатьОбъект("Документ");
	Доки.ВыбратьПодчиненныеДокументы( НачГода(Докум.ДатаДок), КонГода(Докум.ДатаДок), Докум);
	Пока Доки.ПолучитьДокумент() = 1 Цикл
		Если Доки.Вид() = "ВСД_исходящий" Тогда   
			
			Сообщить("Найден "+Доки.ТекущийДокумент()+" "+Доки.Статус);
			ВывестиСекциюВСД(Таб, Доки.ТекущийДокумент());
   			
		ИначеЕсли Доки.Вид() = "ВСД_транзакция" Тогда   
			
			Док2 = СоздатьОбъект("Документ");
			Док2.ВыбратьПодчиненныеДокументы( НачГода(Доки.ДатаДок), КонГода(Доки.ДатаДок), Доки.ТекущийДокумент());
			Пока Док2.ПолучитьДокумент() = 1 Цикл
				Сообщить("Найден "+Док2.ТекущийДокумент()+" "+Док2.Статус);
				ВывестиСекциюВСД_транзакция(Таб, Док2.ТекущийДокумент());
			КонецЦикла;
   		КонецЕсли;   
   		
   	КонецЦикла;
   	
   	Таб.ПараметрыСтраницы(2,100,1,0,0,0,0,0,0,0,0,Устройство);   
   	Таб.ТолькоПросмотр(1);

	
	Если СразуНаПринтер = 0 Тогда
		Таб.Опции(0,0,0,0,);//"ОпцииПечатиРеализация"
		Таб.Показать("Уд. качества "+Докум.НомерДок,"");  

	Иначе     		
		//Таб.КоличествоЭкземпляров(КолвоКопий);
		Таб.Напечатать(0);
	КонецЕсли; 
   
КонецПроцедуры

//******************************************************************************
// ПоКнопкеПечать()
//
// Параметры:
//  Нет
//
// Возвращаемое значение:
//  Нет
//
// Описание:
//
Процедура ПоКнопкеПечать()
	
	Если Док.Выбран() = 0 Тогда
	    Предупреждение("Не выбран документ!", 60);
		Возврат;
	КонецЕсли;
	
	ПечатьСертификат(Док);
	
КонецПроцедуры // ПоКнопкеПечать()

//******************************************************************************
// Предопределенная процедура
//
Процедура ПриОткрытии()
	
	Если ПустоеЗначение(Форма.Параметр) = 0 Тогда
		
		Докум      = Форма.Параметр.Получить("Контекст");
		Устройство = Форма.Параметр.Получить("Устройство");
		КолвоКопий = Форма.Параметр.Получить("КоличествоКопий");
		СразуНаПринтер = Число(Форма.Параметр.Получить("СразуПечать"));
	
		ПечатьСертификат(Докум, Устройство, КолвоКопий, СразуНаПринтер);
		Статусвозврата(0);
		Возврат;
	КонецЕсли;
	
	
КонецПроцедуры // ПриОткрытии()

