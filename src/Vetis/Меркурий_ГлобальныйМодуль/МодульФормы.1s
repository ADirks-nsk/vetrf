Функция ПометитьНаУдалениеСправочник(ВидСправочника) Экспорт
	Состояние("Удаление справочника "+ВидСправочника);
	Спр = СоздатьОбъект("Справочник."+ВидСправочника);
	Спр.ВыбратьЭлементы();
	пока Спр.ПолучитьЭлемент()=1 Цикл
		Спр.Удалить(0);
		Спр.Записать();
	КонецЦикла;
	Состояние("");
КонецФункции

