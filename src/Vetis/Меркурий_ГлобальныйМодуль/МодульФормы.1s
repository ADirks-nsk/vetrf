					RS.УстановитьТекстовыйПараметр("ВыбФирма", ВыбФирма);
				КонецЕсли;
			КонецЕсли;
			ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "%УсловиеПоФирме%", УсловиеПоФирме);

			//условие по складу
			УсловиеПоСкладу = "";
			Если ФильтрПоСкладу = 1 Тогда
