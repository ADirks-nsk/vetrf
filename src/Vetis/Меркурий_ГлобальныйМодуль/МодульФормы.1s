	//		тзАктуальныеПартии.Продукция_Элемент = тзАктуальныеПартии_ПЭ.Продукция_Элемент;
	//		тзАктуальныеПартии.ПолеСортировки = тзАктуальныеПартии_ПЭ.ПолеСортировки;
	//		тзАктуальныеПартии.Количество = тзАктуальныеПартии_ПЭ.Количество;
	//		тзАктуальныеПартии.КоличествоМест = тзАктуальныеПартии_ПЭ.КоличествоМест;
