	КонецЕсли;

    enterprise=objDom.selectSingleNode("//merc:enterprise") ;
	guid = enterprise.selectSingleNode("bs:guid").text;
	uuid = enterprise.selectSingleNode("bs:uuid").text;
	name = enterprise.selectSingleNode("ent:name").text;
