            |            <shp:shipName>"+ СокрЛП(док.номерАвто) +"</shp:shipName>
            |";

            КонецЕсли;
            Запрос=Запрос+"
            |          </shp:transportNumber>
            |        </shp:transportInfo>
            |";
        КонецЕсли;

        Запрос=Запрос+"
        |        <shp:transportStorageType>"+ док.ТермическоеСостояние.Идентификатор() +"</shp:transportStorageType>
