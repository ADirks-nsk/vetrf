	КонецЕсли;
КонецПроцедуры

Процедура ТаблПолеПриРедактированииЗначения(__тп, __стр, __кол, __рег, __знач)
	Если __рег =3 Тогда
		_тз =__тп.ПоставщикДанных.ТаблицаЗначений;
		_нк =__тп.Колонки.Индекс(__кол) ;
		_тз.УстановитьЗначение(__стр, _нк, __знач);
		__тп.ОбновитьСтроки();
	КонецЕсли;
КонецПроцедуры

