		//	_о.ПолучитьЭлемент();
		//	_знач =_о.ТекущийЭлемент();
		//КонецЕсли;

		__тп.РедактироватьЗначение(__стр, __кол, __рег, _знач,,, "ВыбЗнач", "0x100000");

